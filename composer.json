{
    "name": "ashokatree/accuweather",
    "type": "typo3-cms-extension",
    "description": "Current and Forecast weather data by AccuWeather.",
    "version": "10.0.0",
    "keywords": [
        "ashokatree",
        "TYPO3",
        "accuweather",
        "weather"
    ],
    "homepage": "https://ashokatree.net",
    "authors": [
        {
            "name": "Nirmalya Mondal",
            "email": "nirmalya.mondal@gmail.com",
            "role": "Developer"
        }
    ],
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^10.0.0",
        "typo3/cms-scheduler": "^10.0.0"
    },
	"extra": {
		"typo3/cms": {
			"extension-key": "accuweather"
		}
	},
    "autoload": {
        "psr-4": {
            "AshokaTree\\Accuweather\\": "Classes"
        }
    },
    "replace": {
        "ashokatree/accuweather": "self.version",
        "typo3-ter/accuweather": "self.version"
    }
}